    "from scipy import integrate\n",
    "import numpy as np\n",
    "\n",
    "\n",
    "def func(x):\n",
    "    print(x)\n",
    "    f = (np.exp(x / 1) + np.exp(-x / 1)) ** 2\n",
    "    return np.pi * f\n",
    "\n",
    "\n",
    "result = integrate.quad(func, 0, 5)\n",
    "\n",
    "print(result)"
   ]
